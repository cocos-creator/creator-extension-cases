{
    "name": "cocos-panel-html-template",
    "package_version": 2,
    "version": "1.0.0",
    "description": "an extension template for cocos3.0,which use rollup+vue2.x+typescript",
    "main": "dist/main.js",
    "panels": {
        "default": {
            "title": "cocos-panel-html",
            "type": "dockable",
            "main": "dist/panels/default/script"
        }
    },
    "contributions": {
        "menu": [
            {
                "path": "extensions/cocos-panel-html",
                "label": "open cocos-panel-html Panel",
                "message": "open-panel"
            },
            {
                "path": "extensions/cocos-panel-html",
                "label": "default panel say hey",
                "message": "send-to-panel"
            }
        ],
        "messages": {
            "open-panel": {
                "methods": [
                    "open_panel"
                ]
            },
            "send-to-panel": {
                "methods": [
                    "default.hello"
                ]
            }
        }
    },
    "scripts": {
        "build": "node ./build/index.js"
    },
    "keywords": [],
    "author": "Cocos Creator",
    "license": "MIT",
    "devDependencies": {
        "@types/electron": "^1.6.10",
        "@types/fs-extra": "^9.0.6",
        "@types/node": "^14.14.14",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "eslint": "^7.16.0",
        "fs-extra": "^9.0.1",
        "less": "^3.13.0",
        "typescript": "^4.1.3"
    }
}