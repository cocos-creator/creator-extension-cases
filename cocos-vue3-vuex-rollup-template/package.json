{
  "name": "cocos-vue3-vex-rollup-template",
  "package_version": 2,
  "version": "1.0.0",
  "description": "an extension template for cocos3.0,which use rollup+vue3+typescript",
  "main": "dist/main.js",
  "panels": {
    "default": {
      "title": "Vue3 Vex Rollup",
      "type": "dockable",
      "main": "dist/panels/default/main.js"
    },
    "panel1": {
      "title": "Vue3 Vex Rollup",
      "type": "simple",
      "main": "dist/panels/panel1/main.js"
    }
  },
  "contributions": {
    "menu": [
      {
        "path": "extensions/vue3-vex-rollup",
        "label": "open Cocos Rollup Vue3 Panel",
        "message": "open-panel"
      },
      {
        "path": "extensions/vue3-vex-rollup",
        "label": "open Cocos Rollup Vue3 Panel1",
        "message": "open-panel1"
      }
    ],
    "messages": {
      "open-panel": {
        "methods": [
          "open_panel"
        ]
      },
      "open-panel1": {
        "methods": [
          "open_panel1"
        ]
      }
    }
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "watch": "rollup -c ./rollup.config.js -w --configDebug"
  },
  "keywords": [],
  "author": "Cocos Creator",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.0.0",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/parse-json": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "@vue/compiler-sfc": "^3.0.4",
    "eslint": "^7.15.0",
    "eslint-plugin-vue": "^7.2.0",
    "postcss": "^8.2.1",
    "rollup": "^2.34.2",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.1.3",
    "vue-template-compiler": "^2.6.12"
  }
}
