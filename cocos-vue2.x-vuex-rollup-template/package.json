{
  "name": "cocos-vue2.x-vuex-rollup-template",
  "package_version": 2,
  "version": "1.0.0",
  "description": "an extension template for cocos3.0,which use rollup+vue2.x+typescript",
  "main": "dest/main.js",
  "panels": {
    "default": {
      "title": "Vue2.x Vuex Rollup",
      "type": "dockable",
      "main": "dest/panels/default/main.js"
    }
  },
  "contributions": {
    "menu": [
      {
        "path": "extensions/vue2.x-vuex-rollup",
        "label": "open Vue2.x Vuex Rollup Panel",
        "message": "open-panel"
      }
    ],
    "messages": {
      "open-panel": {
        "methods": [
          "open_panel"
        ]
      }
    }
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "watch": "rollup -c ./rollup.config.js -w --configDebug"
  },
  "keywords": [],
  "author": "Cocos Creator",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.12",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@vue/compiler-sfc": "^3.0.4",
    "eslint": "^7.16.0",
    "eslint-plugin-vue": "^7.3.0",
    "postcss": "^8.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^4.1.3",
    "vue-template-compiler": "^2.6.12"
  }
}
